{
  "expo": {
    "name": "tkride",
    "slug": "tkride",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/tkridelogo1.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/tkridelogo1.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tiyokanor32.tkride"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "81a9788a-ab5d-46bd-8f0c-a7352a7c10e4"
      }
    }
  }
}
